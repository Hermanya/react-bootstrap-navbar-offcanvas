{
  "name": "react-bootstrap-navbar-offcanvas",
  "description": "Off-canvas navigation for react using bootstrap",
  "version": "1.0.4",
  "homepage": "https://hermanya.github.io/react-bootstrap-navbar-offcanvas/",
  "main": "lib/navbar-offcanvas.js",
  "jsnext:main": "src/index.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "Hermanya/react-bootstrap-navbar-offcanvas"
  },
  "files": [
    "lib",
    "src"
  ],
  "keywords": [
    "react",
    "ecosystem-react",
    "react-component",
    "react-navbar",
    "navbar-offcanvas",
    "bootstrap-offcanvas",
    "bootstrap"
  ],
  "dependencies": {
    "react": "^16.2.0",
    "react-bootstrap": "^0.31.5",
    "react-dom": "^16.2.0"
  },
  "devDependencies": {
    "autoprefixer": "7.2.5",
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.0",
    "babel-jest": "22.1.0",
    "babel-loader": "7.1.2",
    "babel-preset-react-app": "^3.1.0",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "codecov": "^3.0.0",
    "css-loader": "0.28.9",
    "dotenv": "4.0.0",
    "eslint": "4.16.0",
    "eslint-config-react-app": "^2.0.1",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.41.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.6.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "fs-extra": "3.0.1",
    "gh-pages": "^1.1.0",
    "html-webpack-plugin": "2.29.0",
    "jest": "22.1.3",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.3.0",
    "postcss-loader": "2.0.10",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "react-dev-utils": "^5.0.0",
    "style-loader": "0.20.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.11.1",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3",
    "node-sass-chokidar": "0.0.3"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start": "node scripts/start.js",
    "build": "node scripts/build.js && ./after-build.sh",
    "test": "node scripts/test.js --env=jsdom --coverage",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
